@model DontFretECommerce.Models.Category

@{
    ViewBag.Title = "Browse";
}

    <h2>Product Category:@Model.CategoryName</h2>
    <ul>
        @foreach (var item in Model.products)
        {
            <li>
                @Html.ActionLink(item.ProductName, "Details", new { id = item.ProductID })
            </li>
        }
    </ul>
